            <ui:Label text="0" display-tooltip-when-elided="true" name="HealthPointsValue" class="statValue" style="position: relative; overflow: visible; flex-direction: column;" />
            <ui:Label text="AD: " display-tooltip-when-elided="true" name="AttackDamage" class="valueDescription statName" style="width: 50px;" />
            <ui:Label text="0" display-tooltip-when-elided="true" name="AttackDamageValue" class="statValue" style="position: relative; margin-left: 200px; margin-bottom: 0; padding-bottom: 0;" />
