            <ui:Label text="Items&#10;" display-tooltip-when-elided="true" name="itemTitel" enable-rich-text="false" style="position: relative; color: rgb(255, 255, 255);" />
            <ui:ScrollView scroll-deceleration-rate="0,135" elasticity="0,1" name="items" style="height: 500px; position: absolute; width: 100%; top: 40px; color: rgb(255, 255, 255);" />
